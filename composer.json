{
   "name": "aaix/eloquent-translatable",
   "description": "A clean, performant, and developer-friendly translatable trait for Laravel Eloquent models.",
   "type": "library",
   "license": "MIT",
   "authors": [
      {
         "name": "Jonas Gnioui",
         "email": "jonaaix@icloud.com"
      }
   ],
   "require": {
      "php": "^8.3",
      "illuminate/support": "^10.0 || ^11.0 || ^12.0",
      "illuminate/database": "^10.0 || ^11.0 || ^12.0"
   },
   "require-dev": {
      "orchestra/testbench": "^8.0 || ^9.0",
      "pestphp/pest": "^2.0",
      "pestphp/pest-plugin-laravel": "^2.0"
   },
   "autoload": {
      "psr-4": {
         "Aaix\\EloquentTranslatable\\": "src/"
      }
   },
   "autoload-dev": {
      "psr-4": {
         "Aaix\\EloquentTranslatable\\Tests\\": "tests/"
      }
   },
   "extra": {
      "laravel": {
         "providers": [
            "Aaix\\EloquentTranslatable\\EloquentTranslatableServiceProvider"
         ]
      }
   },
   "config": {
      "allow-plugins": {
         "pestphp/pest-plugin": true
      }
   }
}
